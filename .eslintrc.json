{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "consistent-return": "off",
        "no-restricted-syntax": "off",
        "guard-for-in": "off",
        "no-underscore-dangle": "off"
    }
}
